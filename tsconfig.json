{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "types": ["vue3-toastify/global"],
    "paths": {
      "@/*": ["src/*"]
    },
    "jsx": "preserve"
  }
}
